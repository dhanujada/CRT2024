#include <stdio.h>

int main() {
    // Write C code here
 int a;
 scanf("%d",&a);
 if(a>0){
     printf(" a is positive");
 }
     else{
         if(a<0){
             printf("a is negative");
         }
         else
         { {
                 printf("a is 0");
             }
     }
    return 0;
}
